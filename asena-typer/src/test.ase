name: Option String;

enum Maybe[a] {
    Just(a),
    Nothing;

    fun unwrap(self): a {
        match self {
            Just(value) -> value,
            Nothing -> panic("Called `Maybe.unwrap` in a `Nothing` value")
        }
    }
}

